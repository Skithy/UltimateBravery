{
  "name": "diceroll-online",
  "version": "1.0.0",
  "description": "Simple web app to roll some dice.",
  "main": "src/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skithy/DiceRollOnline.git"
  },
  "scripts": {
    "start": "NODE_ENV=production node_modules/.bin/babel-node --presets 'react,es2015' src/server.js",
