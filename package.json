    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "install": "^0.8.4",
    "npm": "^4.0.5",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.3.1",
    "react-motion": "^0.4.7",
